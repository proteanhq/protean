[flake8]
max-line-length = 119
max-doc-length = 119
extend-ignore = E731, E203
per-file-ignores = __init__.py:F401
exclude =
    .tox,
    .git,
    __pycache__,
    docs/source/conf.py,
    build,
    dist,
    conftest.py,
    *.pyc,
    *.egg-info,
    .cache,
    .eggs
max-complexity = 10
import-order-style = google
application-import-names = flake8
